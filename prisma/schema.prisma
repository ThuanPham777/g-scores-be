generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model student {
  id              String   @id
  math            Float?
  literature      Float?
  foreignlanguage Float?
  physics         Float?
  chemistry       Float?
  biology         Float?
  history         Float?
  geography       Float?
  civics          Float?
  foreignlangcode String?
}