generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Student {
  id             String   @id
  math            Float?
  literature      Float?
  foreignLanguage Float?
  physics         Float?
  chemistry       Float?
  biology         Float?
  history         Float?
  geography       Float?
  civics          Float?
  foreignLangCode String?
}